{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ElasticSearchConfig": {
    "ConnectionString": "http://elasticsearch:9200",
    "PingTimeMilliSeconds": "10000"
  },
