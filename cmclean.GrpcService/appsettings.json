{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Grpc": "Debug"
    }
  },
  "ConnectionStrings": {
    "Default1": "Server=cmclean-db;Port=5432;Database=Contactmanagerdb;User Id=admin;Password=admin1234;",
    "Default": "Server=cmclean-db;Port=5432;Database=Contactmanagerdb;User Id=admin;Password=admin1234;"
  },
  "AppSettings": {
    "ApplicationName": "cmclean.GrpcService",
    "Instance": "2"
  },
  "ExporterSettings": {
    "UseExporter": "jaeger",
    "UseExporterJaeger": "jaeger",
    "UseExporterZipkin": "zipkin"
  },
  "Zipkin": {
    "Endpoint": "http://localhost:9411/api/v2/spans"
  },
  "JaegerSettings": {
    "Host": "http://localhost",
    "Port": "4317",
    "SourceName": "Grpc",
    "ServiceName": "cmclean.GrpcService"
  },
  "RabbitMqConfig": {
    "VHostname": "/",
    "Hostname": "rabbitmq",
    "Username": "rabbitmq",
    "Password": "rabbitmq",
    "Exchange": "LoggerQueue",
    "ExchangeType": "fanout",
    "Port": 5672
  },
  "ConsulConfig": {
    "Address": "http://consul:8500/",
    "ServiceAddress": "http://cmclean-grpc-service:5001",
    "ServiceName": "cmcleanGrpcService",
    "ServiceId": "cmcleanGrpcService"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
